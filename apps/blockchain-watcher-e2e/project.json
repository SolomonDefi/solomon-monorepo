{
  "root": "apps/blockchain-watcher-e2e",
  "sourceRoot": "apps/blockchain-watcher-e2e/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/blockchain-watcher-e2e",
        "main": "apps/blockchain-watcher-e2e/src/main.ts",
        "tsConfig": "apps/blockchain-watcher-e2e/tsconfig.app.json",
        "assets": ["apps/blockchain-watcher-e2e/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/blockchain-watcher-e2e/src/environments/environment.ts",
              "with": "apps/blockchain-watcher-e2e/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "blockchain-watcher-e2e:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/blockchain-watcher-e2e/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/blockchain-watcher-e2e"],
      "options": {
        "jestConfig": "apps/blockchain-watcher-e2e/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
