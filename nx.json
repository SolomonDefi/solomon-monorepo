{
  "npmScope": "monorepo",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-dispute": {
      "tags": ["scope:api", "type:app"]
    },
    "api-evidence": {
      "tags": ["scope:api", "type:app"]
    },
    "blockchain-watcher": {
      "tags": ["scope:service", "type:app"]
    },
    "blockchain-watcher-e2e": {
      "tags": ["scope:service", "type:app"]
    },
    "blockchain-watcher-feature-templates": {
      "tags": ["scope:service", "type:feature"]
    },
    "contracts": {
      "tags": ["scope:blockchain", "type:app"]
    },
    "db": {
      "tags": ["scope:db", "type:app"]
    },
    "developer-docs": {
      "tags": ["scope:web", "type:docs"]
    },
    "lib-blockchain-watcher": {
      "tags": ["scope:service", "type:lib"]
    },
    "plugin": {
      "tags": ["scope:web", "type:plugin"]
    },
    "plugin-example-vue3": {
      "tags": ["scope=web", "type=app"]
    },
    "shared-util-api-evidence-types": {
      "tags": ["scope:shared", "type:util"]
    },
    "shared-util-core": {
      "tags": ["scope:shared", "type:util"]
    },
    "shared-util-i18n": {
      "tags": ["scope:shared", "type:util"]
    },
    "shared-util-path-store": {
      "tags": ["scope:shared", "type:util"]
    },
    "ui-theme": {
      "tags": ["scope:web", "type:ui"]
    },
    "ui-widgets": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-data-access-api": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-data-access-state": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-evidence": {
      "tags": ["scope:web", "type:app"]
    },
    "web-ui-assets": {
      "tags": ["scope:web", "type:ui"]
    }
  }
}
